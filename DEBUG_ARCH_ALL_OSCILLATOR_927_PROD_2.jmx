          <stringProp name="script">/**
 * This script generates two variables, &quot;stream&quot; is a string representing the body content of a random stream, 
 * and &quot;hash&quot; is a base-64 encoded string of the SHA256 hash of that stream body.
 */


import java.security.MessageDigest;
import javax.xml.bind.DatatypeConverter;

MessageDigest digest = (MessageDigest) vars.getObject(&quot;digest&quot;);
if(digest == null){
	digest = MessageDigest.getInstance(&quot;SHA-256&quot;);
	vars.putObject(&quot;digest&quot;,digest);	
}
digest.reset();

Random random = (Random) vars.getObject(&quot;random&quot;);
if(random == null){
	random = new Random();
	vars.putObject(&quot;random&quot;,random);
}

String stream = &quot;This is the body of the stream with a random number &quot;.concat(String.valueOf(random.nextLong()));
byte[] hash = digest.digest(stream.getBytes(&quot;UTF-8&quot;));
String hashString = DatatypeConverter.printBase64Binary(hash).replaceAll(&quot;\\+&quot;,&quot;-&quot;).replaceAll(&quot;/&quot;,&quot;_&quot;).replaceAll(&quot;=&quot;,&quot;&quot;);

vars.put(&quot;hash&quot;,hashString);
vars.put(&quot;stream&quot;,stream);
