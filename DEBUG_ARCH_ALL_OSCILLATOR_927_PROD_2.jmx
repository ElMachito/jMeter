	byte[] hash = digest.digest(builder.toString().getBytes(&quot;UTF-8&quot;));
	
	vars.put(&quot;sig&quot;,DatatypeConverter.printHexBinary(hash));
	vars.put(&quot;sigTime&quot;,curTime);
